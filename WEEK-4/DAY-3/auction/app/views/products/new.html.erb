<h1>Add new product</h1>

<%= form_for @product do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br><br>

  <%= f.label :enddate %><br>
  <%= f.date_field :enddate %><br><br>

  <%= f.label :user_id %><br>
  <%= f.text_field :user_id %><br><br>

  <%= f.button "Add product" %>
<% end %>
